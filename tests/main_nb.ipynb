   "source": [
    "async def main():\n",
    "    input_str = \"能不能告诉我杭州今天的天气状况?\"\n",
    "    result = await Runner.run(\n",
    "        starting_agent=starting_agent, \n",
    "        input=input_str\n",
    "    ) \n",
    "    rprint(result.final_output)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
